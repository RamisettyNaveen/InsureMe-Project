pipeline {
    agent any
    
    tools {
        maven 'M2_HOME'
    }
    
    stages {
        stage ('Git checkout') {
            steps {
                echo 'Cloning the git repo'
                git branch: 'main', url: 'https://github.com/RamisettyNaveen/InsureMe-Project.git'
            }
        }
        
        stage('Package') {
            steps {
                echo 'Generate a jar file by packaging using Maven'
                sh 'mvn clean package'
            }
        }
        
        stage('Publish TestNG report') {
            steps {
                echo 'Generate a TestNG report'
                  
              
            }
        }
    }
}
